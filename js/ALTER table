ALTER TABLE mytable
	ADD COLUMN boro text,
	ADD COLUMN sommdistrict text;

UPDATE mytable
	SET
		boro = support_admin_boroboundaries_wi.boroname,
		commdistrict = 
	FROM (
		SELECT
			mytable.*,

		FROM
			mytable
		LEFT JOIN support_admin_boroboundaries_wi
			ON ST_Intersects(support_admin_boroboundaries_wi.the_geom, mytable.the_geom)
		LEFT JOIN support_admin_cdboundaries
			ON ST_Intersects(support_admin_cdboundaries.the_geom, mytable.the_geom)
			);
